{
  "title": "Notification Model - List",
  "description": "Schema definition for Notification Model - list method",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "walletId": {
      "description": "The wallet ID",
      "type": "string"
    },
    "fromTimestamp": {
      "description": "Time and date of notification",
      "type": "string",
      "pattern": "^[0-9]{4}-(0[1-9]|1[0-2])-(0[1-9]|[1-2][0-9]|3[0-1])T(2[0-3]|[01][0-9]):[0-5][0-9]:[0-5][0-9](.[0-9]+)?(([+-]([01][0-4]):[0-5][0-9])|Z)?$"
    },
    "type": {
      "description": "The notification type",
      "type": "string"
    },
    "fetchLatest": {
      "description": "A flag for fetching latest notification",
      "type": "boolean"
    }
  },
  "required": [
    "walletId"
  ]
}
