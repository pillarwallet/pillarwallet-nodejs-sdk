{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Notification Class - Search",
  "description": "Schema definition for Notification Class - List method",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "walletId": {
      "description": "The wallet ID",
      "type": "number",
      "exclusiveMinimum": 0
    },
    "fromTimestamp": {
      "description": "Time and date of notification",
      "type": "string",
      "pattern": "^[0-9]{4}-(0[1-9]|1[0-2])-(0[1-9]|[1-2][0-9]|3[0-1])T(2[0-3]|[01][0-9]):[0-5][0-9]:[0-5][0-9](.[0-9]+)?(([+-]([01][0-4]):[0-5][0-9])|Z)?$"
    }
  },
  "required": [
    "walletId",
    "fromTimestamp"
  ]
}
